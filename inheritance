class Animal {
    String name;
    
    void makeSound() {
        System.out.println("Some sound");
    }
}

class Dog extends Animal {
    Dog(String name) {
        this.name = name;
    }
    
    void makeSound() {
        System.out.println("Bark");
    }
}

public class InheritanceExample {
    public static void main(String[] args) {
        Dog dog = new Dog("Buddy");
        System.out.println(dog.name);
        dog.makeSound();
    }
}
